<html>
<head>
	
</head>
<body>
	<!-- Device Login -->
	<ul>
    <% if user_signed_in? %>
    	<h2> USER signed in!</h2>
        <li><%= link_to 'Edit Profile', edit_user_registration_path%></li>
        <li><%= link_to 'Log Out', destroy_user_session_path, method: :delete %></li>
    <% elsif admin_signed_in? %>
    	<h2> Your ADMIN ID: <%= current_admin.id %></h2>
    	<li><%= link_to 'Edit Profile', edit_admin_registration_path%></li>
    	<li><%= link_to 'Log Out', destroy_admin_session_path, method: :delete%></li>
        <li><%= link_to 'Make New Project', new_project_path%></li>
        <li><%= link_to 'Your Current Projects', projects_path(:adminID=>current_admin.id) %></li>
        <% end %>
    <% else %>
        <li><%= link_to 'Student Log In', new_user_session_path %></li>
        <li><%= link_to 'Student Sign Up', new_user_registration_path %></li>
        <li><%= link_to 'Project Manager Log In', new_admin_session_path%></li>
        <li><%= link_to 'Project Manager Sign Up', new_admin_registration_path%></li>
    <% end %>
    </ul>
</body>
</html>